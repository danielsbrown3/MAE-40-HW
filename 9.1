%Defines s as a transfer function
s= tf('s')

%Computes the pade approximation of e^(-ds) when d=.1 and n=1
%n=1
g1=pade(exp(-.1*s),1);
figure
grid on 
hold on
%Plots the roots of the numerator as red cirles
plot(roots([-1 20]),'o','color','red','MarkerSize',20)
%PLots the roots of the denominator as blue x's
plot(roots([1 20]),'x','color','blue','MarkerSize',20)

%Computes the pade approximation of e^(-ds) when d=.1 and n=2
%n=2
g2=pade(exp(-.1*s),2);
