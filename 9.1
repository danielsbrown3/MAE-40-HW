clf;clc;clear all;
%Defines s as a transfer function
s= tf('s')

%Computes the pade approximation of e^(-ds) when d=.1 and n=1
%n=1
g1=pade(exp(-.1*s),1)
pzplot(tf([-1 20],[1 20]))

%Computes the pade approximation of e^(-ds) when d=.1 and n=2
%n=2
g2=pade(exp(-.1*s),2)
pzplot(tf([1 -60 1200],[1 60 1200]))

%Computes the pade approximation of e^(-ds) when d=.1 and n=4
%n=4
g3=pade(exp(-.1*s),4)
pzplot(tf([1 -200 18000 -8.4e05 1.68e07],[1 200 18000 8.4e05 1.68e07]))

%Computes the pade approximation of e^(-ds) when d=.1 and n=8
%n=8
g4=pade(exp(-.1*s),8)
pzplot(tf([1 -720 252000 -5.544e07 8.316e09 -8.649e11 6.054e13 -2.595e15 5.189e16],[1 720 252000 5.544e07 8.316e09 8.649e11 6.054e13 2.595e15 5.189e16]))

